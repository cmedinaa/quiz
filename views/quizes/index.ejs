<form name="search-form" action="/quizes" method="get">
	<input type="text" name="search" placeholder="Search question" /><br>
	<input type="submit" value="Buscar" />
</form>
<br>
<table>
<% var i; for (i=0; i<quizes.length; i++) {%>
	<tr>
		<td>
			<a href="quizes/<%=quizes[i].id %>"> <%=quizes[i].pregunta %></a>
		</td>
	</tr>
<% } %>
</table>
  